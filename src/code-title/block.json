{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "wporg/code-reference-title",
	"version": "0.1.0",
	"title": "Code Reference: Title",
	"category": "widgets",
	"icon": "smiley",
	"description": "Show the code signature for a function, class, or method.",
	"usesContext": [ "postId" ],
	"attributes": {
		"isLink": {
			"type": "boolean",
			"default": false
		},
		"tagName": {
			"type": "string",
			"default": "h1"
		}
	},
	"supports": {
		"html": false,
		"spacing": {
			"margin": [
				"top",
				"bottom"
			],
			"padding": true
		},
		"typography": {
			"fontSize": true,
			"lineHeight": true
		}
	},
	"textdomain": "wporg",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css"
}
