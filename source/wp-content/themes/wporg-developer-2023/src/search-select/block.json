{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "wporg/search-select",
	"title": "Search Select",
	"category": "layout",
	"description": "Displays a selector to choose which content to search.",
	"keywords": [ "post" ],
	"textdomain": "wporg",
	"attributes": {
		"hideLabelFromVision": {
			"type": "boolean",
			"default": false
		},
		"label": {
			"type": "string",
			"default": ""
		}
	},
	"supports": {
		"html": false,
		"color": {
			"text": true,
			"background": true
		},
		"spacing": {
			"margin": [
				"top",
				"bottom"
			]
		},
		"typography": {
			"fontSize": true
		}
	},
	"editorScript": "file:./index.js",
	"style": "file:./style-index.css",
	"viewScript": "file:./view.js"
}
