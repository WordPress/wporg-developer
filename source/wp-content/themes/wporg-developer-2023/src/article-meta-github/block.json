{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "wporg/article-meta-github",
	"version": "0.1.0",
	"title": "Article Meta GitHub",
	"category": "widgets",
	"icon": "smiley",
	"description": "",
	"usesContext": [ "postId", "postType" ],
	"attributes": {
		"heading": {
			"type": "string",
			"default": ""
		},
		"linkText": {
			"type": "string",
			"default": ""
		},
		"linkUrl": {
			"type": "string",
			"default": ""
		}
	},
	"supports": {
		"inserter": false
	},
	"textdomain": "wporg",
	"editorScript": "file:./index.js"
}
