{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "wporg/code-reference-source",
  "version": "0.1.0",
  "title": "Code Reference: Source",
  "category": "widgets",
  "icon": "smiley",
  "description": "Show the code source for a function, class, or method.",
  "usesContext": [
    "postId"
  ],
  "supports": {
    "html": false,
    "spacing": {
      "margin": [
        "top",
        "bottom"
      ],
      "padding": true
    },
    "typography": {
      "fontSize": true,
      "lineHeight": true
    }
  },
  "textdomain": "wporg",
  "editorScript": "file:./index.js"
}