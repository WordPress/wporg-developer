{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "wporg/code-reference-summary",
  "version": "0.1.0",
  "title": "Code Reference: Summary",
  "category": "widgets",
  "icon": "smiley",
  "description": "Code summary for a function, method, or class.",
  "usesContext": [
    "postId"
  ],
  "supports": {
    "html": false,
    "spacing": {
      "margin": [
        "top",
        "bottom"
      ],
      "padding": true
    },
    "typography": {
      "fontSize": true,
      "lineHeight": true
    }
  },
  "textdomain": "wporg",
  "editorScript": "file:./index.js",
  "style": "file:./style-index.css"
}